using System;

namespace Test_OOP
{
    public class Tellme 
    {
        public void Name(string a)
        {
            Console.WriteLine("Hello "+a);
        }
        public void Name(string a, string b)
        {
            Console.WriteLine("Hello "+ a + " " + b);
        }
        public void Name(string a, string b, int c)
        {
            Console.WriteLine("Hello " + a + " " + b + "Your old = " + c );
        }
    }
    class Program
    {
        static void Main(string[] arg)
        {
            Tellme tell = new Tellme();
            tell.Name("Valera");
            tell.Name("Valera","Kulikov");
            tell.Name("Valera","Kulikov", 18);
            Console.ReadKey();
        }


    }
        
}
